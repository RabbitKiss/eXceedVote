<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="exceedVote">
	<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<class>model.Testtable</class>
		<class>model.Temp</class>
		<!-- <class>model.Ballottype</class> -->
		<class>exceedvote.domain.BallotType</class>
		<class>exceedvote.domain.ScoreType</class>
		<class>exceedvote.domain.Choice</class>
		<class>exceedvote.domain.User</class>
	<properties> 
       <property name="javax.persistence.jdbc.url" 
                 value="jdbc:mysql://localhost:3306/exceedvote"/> 
       <property name="javax.persistence.jdbc.driver" 
    value="com.mysql.jdbc.Driver"/> 
       <property name="javax.persistence.jdbc.password" value="Whiterabbit"/> 
       <property name="javax.persistence.jdbc.user" value="root"/> 
    </properties>
	</persistence-unit>
</persistence>
